# Example .env file for AI Content Generation Pipeline
# Copy this file to .env and fill in your actual values

# DeepSeek API Key (Required)
# Get your key from: https://platform.deepseek.com/
DEEPSEEK_API_KEY=your_deepseek_api_key_here

# Optional: Serper API Key for enhanced search capabilities
# Get your key from: https://serper.dev/
SERPER_API_KEY=your_serper_api_key_here

# Authentication (Mandatory for production)
# Set ENABLE_AUTH to 'true' to enable login
ENABLE_AUTH=true
APP_USERNAME=admin
APP_PASSWORD=admin
APP_SECRET_KEY=replace_this_with_a_long_random_string
